{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/register/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/posts/create/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/register/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}